if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    not_toplam = 0
    for i in student_marks[query_name]:
        not_toplam += i
    ortalama = f"{(not_toplam/len(student_marks[query_name])):.2f}"
    print(ortalama)
